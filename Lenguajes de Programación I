#include <iostream>

int main() {
    int edad;

    // Solicitar el ingreso de la edad
    std::cout << "Ingrese su edad: ";
    std::cin >> edad;

    // Determinar si es mayor o menor de edad
    if (edad >= 18) {
        std::cout << "Usted es mayor de edad." << std::endl;
    } else {
        std::cout << "Usted es menor de edad." << std::endl;
    }

    return 0;
}
